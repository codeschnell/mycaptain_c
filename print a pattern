#include<stdio.h>
#include<conio.h>
void main()

{
    int i,j,k=0;
    for(i=2;i<=7;i++)
    {
        for(j=1;j<=i;j++)
        {
            if(k%2==0)
            {
                printf("0");
            }
            else
            {
                printf("1");
            }
            k++;
        }
        printf("\n");
    }
    getch();
}
